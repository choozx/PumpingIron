	 function kakaoLogout() {
		    if (Kakao.Auth.getAccessToken()) {
		      //토큰이 있으면
		      Kakao.API.request({
		        //로그아웃하고
		        url: '/v1/user/unlink',
		        success: function (response) {
		      	//토큰도 삭제
		     	 	Kakao.Auth.setAccessToken(undefined)
			  	 //	alert(response.access_token);
		         // alert(response)
		        // 로그아웃 시키기
		         location.href="member.logout"
		        },
		        fail: function (error) {
		          console.log(error)
		        },
		      })
		    }
	  }
